#prints grade based on score
#uses function

def computegrade():
    rawscore = raw_input("Please enter your score of 0.0 to 1.0")
    score = float(rawscore)

    if score > 1.0 :
        print "Bad score"
    elif score >= 0.9 :
        print "A"
    elif score >= 0.8 :
        print "B"
    elif score >= 0.7 :
        print "C"
    elif score >= 0.6 :
        print "D"
    elif score < 0.0 :
        print "Bad score"
    elif score < 0.6 :
        print "F"
    return score
        
computegrade()
